{
  "name": "@kiebitz-oss/config",
  "description": "Kiebitz Web Applications",
  "version": "1.0.0",
  "author": "Benjamin Bender <bb@codepoet.de>",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "main": "src/index.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/kiebitz-oss/apps-ffm",
    "directory": "packages/config"
  },
  "bugs": "https://github.com/kiebitz-oss/apps-ffm/issues",
  "engines": {
    "node": "^14.18.1 || >=16.0.0"
  },
  "scripts": {
    "clean": "rimraf --no-glob ./dist .tsbuildinfo",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:staged": "lint-staged --allow-empty",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-testing-library": "^5.0.1",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/*": "yarn lint --fix"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "../../eslintrc.base.js"
    ],
    "plugins": [
      "testing-library"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "overrides": [
      {
        "files": [
          "**/*.test.{ts,tsx}"
        ],
        "extends": [
          "plugin:testing-library/react"
        ]
      }
    ]
  }
}
