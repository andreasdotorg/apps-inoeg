{
  "name": "@kiebitz-oss/ui",
  "description": "Kiebitz Web Applications",
  "version": "1.0.0",
  "author": "Benjamin Bender <bb@codepoet.de>",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "main": "src/index.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/kiebitz-oss/apps-ffm",
    "directory": "packages/ui"
  },
  "bugs": "https://github.com/kiebitz-oss/apps-ffm/issues",
  "engines": {
    "node": "^14.18.1 || >=16.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.4",
    "react-use": "^17.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addons": "^6.4.9",
    "@storybook/client-api": "^6.4.9",
    "@storybook/preview-web": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@storybook/testing-react": "^1.2.2",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "babel-jest": "^27.4.5",
    "jest": "^27.4.5",
    "next": "^12.0.7",
    "typescript": "^4.5.4"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not op_mini all"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom",
      "@testing-library/jest-dom/extend-expect",
      "jest-axe/extend-expect"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.next/",
      "<rootDir>/dist/"
    ]
