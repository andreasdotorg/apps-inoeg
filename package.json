{
  "name": "kiebitz-ffm",
  "description": "Kiebitz Web Applications",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "contributors": [
    "Benjamin Bender <bb@codepoet.de>",
    "Andreas Dewes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiebitz-oss/apps-ffm"
  },
  "bugs": "https://github.com/kiebitz-oss/apps-ffm/issues",
  "engines": {
    "node": "^14.18.1 || >=16.0.0",
    "pnpm": "^6"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "changeset": "changeset",
    "build": "pnpm run build --recursive --if-present",
    "build:user": "pnpm build --filter ./apps/user",
    "build:mediator": "pnpm build --filter ./apps/mediator",
    "build:provider": "pnpm build --filter ./apps/provider",
    "build:sb": "pnpm build:sb --filter './common/storybook'",
    "build:i18n": "pnpm run i18n:build --parallel --recursive --if-present",
    "dev:user": "pnpm dev --filter ./apps/user",
    "dev:provider": "pnpm dev --filter ./apps/provider",
    "dev:mediator": "pnpm dev --filter ./apps/mediator",
    "dev:sb": "pnpm dev --filter ./common/storybook",
    "clean": "pnpm run clean --parallel --recursive --if-present",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:staged": "lint-staged --allow-empty",
    "lint:css": "npx stylelint '**/*.css'",
    "typecheck": "pnpm run typecheck --parallel --recursive --if-present",
    "test": "pnpm run test --parallel --recursive --if-present",
    "test:e2e": "start-server-and-test test:e2e:serve http-get://localhost:3000 'cypress open'",
    "test:e2e:serve": "lingui compile & webpack serve --mode=development",
    "release": "pnpm build && changeset publish",
    "reset": "rm -rf node_modules/ common/storybook/node_modules/ common/ui/node_modules/ common/config/node_modules/ common/api/node_modules/ apps/mediator/node_modules/ apps/provider/node_modules/ apps/user/node_modules/",
    "docker:build:user": "docker build . -f Dockerfile-user -t kiebitz-user",
    "docker:build:mediator": "docker build . -f Dockerfile-mediator -t kiebitz-mediator",
    "docker:build:provider": "docker build . -f Dockerfile-provider -t kiebitz-provider",
    "docker:user": "docker run -p 3000:3000 -it kiebitz-user",
    "docker:mediator": "docker run -p 3000:3000 -it kiebitz-mediator",
    "docker:provider": "docker run -p 3000:3000 -it kiebitz-provider"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@changesets/changelog-github": "^0.4.2",
    "@changesets/cli": "^2.19.0",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.5",
    "@types/tailwindcss": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "autoprefixer": "^10.4.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.2.0",
    "cypress-fail-fast": "^3.3.0",
    "eslint": "^8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-storybook": "^0.5.5",
    "eslint-plugin-tailwindcss": "^3.0.2",
    "eslint-plugin-testing-library": "^5.0.1",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.1.0",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.2.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "tailwindcss": "^3.0.8",
    "typescript": "^4.5.4",
    "webpack": "^5.0.0"
  },
  "resolutions": {
    "ansi-regex": "^5",
    "browserslist": "^4.16",
    "immer": "^9",
    "glob-parent": "^6",
    "trim": "^1",
    "webpack": "^5"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  },
  "postcss": {
    "plugins": {
      "tailwindcss/nesting": {},
      "tailwindcss": {},
      "postcss-flexbugs-fixes": {},
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "nesting-rules": false,
          "custom-properties": false
        }
      }
    }
  },
  "dependencies": {
    "next": "^12.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
