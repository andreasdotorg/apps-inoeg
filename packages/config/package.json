{
  "name": "@kiebitz-oss/config",
  "description": "Kiebitz Web Applications",
  "version": "1.0.0",
  "author": "Benjamin Bender <bb@codepoet.de>",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "source": "src/index.ts",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "src/assets/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiebitz-oss/apps-ffm",
    "directory": "packages/config"
  },
  "bugs": "https://github.com/kiebitz-oss/apps-ffm/issues",
  "engines": {
    "node": "^14.18.1 || >=16.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "bundle": "microbundle --tsconfig ./tsconfig.build.json -f cjs,es",
    "build": "rimraf --no-glob ./dist && cross-env NODE_ENV=production yarn run bundle --compress",
    "dev": "yarn run bundle watch --no-compress",
    "clean": "rimraf --no-glob ./dist",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests"
  },
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.4.5",
    "microbundle": "^0.14.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not op_mini all"
  ]
}
